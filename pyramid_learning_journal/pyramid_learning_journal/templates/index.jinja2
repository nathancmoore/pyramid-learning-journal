{% extends "layout.jinja2" %}

        {% block content %}
            {% for entry in entries %}
        <div class="journal_entry">
            <h2 class="entry_title"><a href="{{request.route_url('details', id=entry.id)}}">{{entry.title}}</a></h2>
            <p class="entry_date">{{entry.date}}</p>
        </div>
        {% endfor %}
        {% endblock %}